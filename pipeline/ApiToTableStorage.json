{
	"name": "ApiToTableStorage",
	"properties": {
		"activities": [
			{
				"name": "ApiToTableStorage",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureTableSink",
						"azureTableInsertType": "replace",
						"azureTableRowKeyName": "ID",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"name": "ID"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_name']"
								},
								"sink": {
									"name": "Name"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_salary']"
								},
								"sink": {
									"name": "Salary"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_age']"
								},
								"sink": {
									"name": "Age"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['profile_image']"
								},
								"sink": {
									"name": "Image"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "API",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Employees",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}